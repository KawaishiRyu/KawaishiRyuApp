<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >

    </fragment>

    <fragment
        android:id="@+id/locationFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.location.LocationFragment"
        android:label="fragment_location"
        tools:layout="@layout/fragment_location" >
        <action
            android:id="@+id/action_locationFragment_to_registerDojoFragment"
            app:destination="@id/registerDojoFragment" />

        <action
            android:id="@+id/action_locationFragment_to_detailLocationFragment"
            app:destination="@id/detailLocationFragment" />

    </fragment>

    <fragment
        android:id="@+id/registerDojoFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.location.RegisterDojoFragment"
        android:label="fragment_register_dojo"
        tools:layout="@layout/fragment_register_dojo" >
        <argument
            android:name="dojoModel"
            app:argType="app.kawaishiryu.jiujitsu.data.model.dojos.DojosModel"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>

    <fragment
        android:id="@+id/profileUserFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.profile.ProfileUserFragment"
        android:label="fragment_profile_user"
        tools:layout="@layout/fragment_profile_user" >
        <action
            android:id="@+id/action_profileUserFragment_to_profileUserModifiedFragment"
            app:destination="@id/profileUserModifiedFragment" />
        <argument
            android:name="changeData"
            app:argType="boolean"
            android:defaultValue="true" />
        <action
            android:id="@+id/action_profileUserFragment_to_menuJiuJitsuFragment"
            app:destination="@id/menuJiuJitsuFragment" />
    </fragment>

    <dialog
        android:id="@+id/dialogSignOutUser"
        android:name="app.kawaishiryu.jiujitsu.view.DialogSignOutUser"
        android:label="DialogSignOutUser" />
    <fragment
        android:id="@+id/profileUserModifiedFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.profile.ProfileUserModifiedFragment"
        android:label="fragment_profile_user_modified"
        tools:layout="@layout/fragment_profile_user_modified" >
        <action
            android:id="@+id/action_profileUserModifiedFragment_to_profileUserFragment"
            app:destination="@id/profileUserFragment" />
        <argument
            android:name="currentUserModifed"
            app:argType="app.kawaishiryu.jiujitsu.data.model.CurrentUser" />
    </fragment>

    <fragment
        android:id="@+id/timeRegisterFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.location.TimeRegisterFragment"
        android:label="fragment_time_register"
        tools:layout="@layout/fragment_time_register" />

    <fragment
        android:id="@+id/menuTecFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.techniques_menu.MenuTecFragment"
        android:label="fragment_menu_tec"
        tools:layout="@layout/fragment_menu_tec" >
        <action
            android:id="@+id/action_menuTecFragment_to_tecnicasFragment"
            app:destination="@id/tecnicasFragment" />

    </fragment>


    <fragment
        android:id="@+id/tecnicasFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.techniques_menu.TecnicasFragment"
        android:label="fragment_tecnicas"
        tools:layout="@layout/fragment_tecnicas" >
        <argument
            android:name="nameTec"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="nameTecSec"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_tecnicasFragment_to_createTecFragment"
            app:destination="@id/createTecFragment" />
        <action
            android:id="@+id/action_tecnicasFragment_to_detailTecFragment"
            app:destination="@id/detailTecFragment" />
        <argument
            android:name="tanslateTec"
            app:argType="string"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/detailLocationFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.location.DetailLocationFragment"
        android:label="fragment_detail_location"
        tools:layout="@layout/fragment_detail_location" >
        <argument
            android:name="dojoDetail"
            app:argType="app.kawaishiryu.jiujitsu.data.model.dojos.DojosModel" />
        <action
            android:id="@+id/action_detailLocationFragment_to_timeRegisterFragment"
            app:destination="@id/timeRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/createTecFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.techniques_menu.CreateTecFragment"
        android:label="fragment_create_tec"
        tools:layout="@layout/fragment_create_tec" >
        <argument
            android:name="path_fb"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/detailTecFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.techniques_menu.DetailTecFragment"
        android:label="fragment_detail_tec"
        tools:layout="@layout/fragment_detail_tec" >
        <argument
            android:name="modelMovArg"
            app:argType="app.kawaishiryu.jiujitsu.data.model.movimientos.MoviemientosModel"
            app:nullable="true" />
        <argument
            android:name="nameMovArg"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/menuJiuJitsuFragment"
        android:name="app.kawaishiryu.jiujitsu.ui.fragment.techniques_menu.MenuJiuJitsuFragment"
        android:label="fragment_menu_jiu_jitsu"
        tools:layout="@layout/fragment_menu_jiu_jitsu" />
</navigation>